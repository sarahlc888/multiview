# Copy this file to .env and fill in your values
# .env is gitignored and will not be committed

# ============================================================================
# API Keys
# ============================================================================

# OpenAI - supports single key or comma-separated list for load balancing
# OPENAI_API_KEY=sk-...
# OPENAI_API_KEYS=sk-key1,sk-key2,sk-key3

# Anthropic (Claude)
# ANTHROPIC_API_KEY=sk-ant-...

# Google Gemini
# GEMINI_API_KEY=...
# GOOGLE_API_KEY=...  # Alternative name

# HuggingFace API
# HF_TOKEN=hf_...
# HF_API_KEY=hf_...  # Alternative name
# HUGGINGFACE_API_KEY=hf_...  # Alternative name

# ============================================================================
# Cache Settings
# ============================================================================

# Global cache on/off switch (default: true)
# USE_CACHE=true
